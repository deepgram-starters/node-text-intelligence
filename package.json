{
  "name": "@deepgram/node-text-intelligence",
  "version": "1.0.0",
  "private": true,
  "description": "Text Intelligence starter app using Deepgram's Read API for summarization, topics, sentiment, and intent analysis",
  "main": "server.js",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
