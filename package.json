  "dependencies": {
    "@deepgram/sdk": "5.0.0-beta.4",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "express": "5.2.1",
    "jsonwebtoken": "9.0.2",
    "toml": "3.0.0"
  },
  "devDependencies": {
    "nodemon": "3.1.10"
  },
  "browserslist": {
